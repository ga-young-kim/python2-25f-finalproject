      <item>
        <titleType>Official Title as Introduced</titleType>
        <titleTypeCode>6</titleTypeCode>
        <title>To prohibit the United States Government from providing bailouts or other financial assistance to any State or local government that enacts any law providing reparations for slavery, and for other purposes.</title>
        <updateDate>2024-03-18T21:03:48Z</updateDate>
        <billTextVersionName>Introduced in House</billTextVersionName>
        <billTextVersionCode>IH</billTextVersionCode>
