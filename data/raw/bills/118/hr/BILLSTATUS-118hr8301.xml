        <title>United States Call Center Worker and Consumer Protection Act of 2024</title>
        <updateDate>2024-06-11T03:08:15Z</updateDate>
        <billTextVersionName>Introduced in House</billTextVersionName>
        <billTextVersionCode>IH</billTextVersionCode>
